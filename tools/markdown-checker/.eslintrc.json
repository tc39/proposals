{
    "env": {
        "node": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
      "no-underscore-dangle": 0,
      "import/no-extraneous-dependencies": 0,
      "no-unused-vars": 0
    }
}
